//strstr--查找字符串

char* my_strstr(const char* p1, const char* p2)
{
	assert(p1 != NULL);
	assert(p2 != NULL);
	char* s1 = NULL;
	char* s2 = NULL;
	char* cur = p1;

	if (*p2 == '\0')
	{
		return (char*)p1;
	}
	while (*cur)
	{
		s1 = cur;
		s2 = (char*)p1;
		while ((*s2!='\0')&&(*s1 != '\0')&& (*s1 == *s2))
		{
			s1++;
			s2++;
		}
		if (*s2 == '\0')
		{
			return cur;					//找到了字符串
		}
		cur++;
	}
	return NULL;						//找不到字符串
}
int main()
{
	char* p1 = "abcdefabcdef";
	char* p2 = "def";
	//char* ret=strstr(p1, p2);
	char* ret=my_strstr(p1, p2);

	if (ret == NULL)
	{
		printf("子串不存在\n");
	}
	else
	{
		printf("%s\n", ret);
	}
	return 0;
}
